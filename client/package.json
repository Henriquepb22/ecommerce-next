{
  "name": "react-avancado-boilerplate",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint src --max-warnings=0",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "test": "jest",
    "test:watch": "yarn test --watch",
    "schema:download": "apollo client:download-schema --endpoint=http://localhost:1337/graphql schema-temp.gql",
    "schema:convert": "node convertToNonNull.js",
    "codegen": "apollo codegen:generate --target typescript --excludes=node_modules/ --includes=src/graphql/**/*.ts --localSchemaFile schema.gql --tagName=gql --outputFlat src/graphql/generated",
    "types:generate": "yarn schema:download && yarn schema:convert && yarn codegen",
    "generate": "yarn plop --plopfile generators/plopfile.js",
    "storybook": "start-storybook -s ./public -p 6006",
    "build-storybook": "build-storybook -s ./public"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*": [
      "yarn lint --fix",
      "yarn test --findRelatedTests --bail"
    ]
  },
  "dependencies": {
    "@apollo/client": "^3.3.12",
    "@styled-icons/boxicons-regular": "^10.23.0",
    "@styled-icons/boxicons-solid": "^10.23.0",
    "@styled-icons/fa-brands": "^10.26.0",
    "@styled-icons/material-outlined": "^10.28.0",
    "@styled-icons/remix-fill": "^10.18.0",
    "@types/lodash.xor": "^4.5.6",
    "graphql": "^15.5.0",
    "joi": "^17.6.0",
    "lodash.xor": "^4.5.0",
    "next": "10.0.7",
    "next-auth": "^3.27.0",
    "next-pwa": "^3.1.4",
    "nextjs-progressbar": "^0.0.14",
    "polished": "^4.0.5",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-slick": "^0.28.0",
    "styled-components": "5.3.0",
    "styled-media-query": "^2.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@babel/preset-typescript": "^7.10.4",
    "@storybook/addon-essentials": "6.0.21",
    "@storybook/react": "6.0.21",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.0.4",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/user-event": "^12.7.1",
    "@types/jest": "^27.4.1",
    "@types/next-auth": "^3.15.0",
    "@types/node": "^17.0.21",
    "@types/react": "^17.0.39",
    "@types/react-slick": "^0.23.4",
    "@types/styled-components": "^5.1.3",
    "@typescript-eslint/eslint-plugin": "^4.5.0",
    "@typescript-eslint/parser": "^4.5.0",
    "apollo": "^2.32.5",
    "babel-loader": "^8.1.0",
    "babel-plugin-styled-components": "^1.11.1",
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^4.3.0",
    "jest": "^27.5.1",
    "jest-styled-components": "^7.0.3",
    "lint-staged": "^10.5.4",
    "plop": "^2.7.4",
    "prettier": "^2.2.1",
    "typescript": "^4.0.3"
  }
}
